{
    // "rust-analyzer.cargo.target": "x86_64-rrtos.json",
    "rust-analyzer.cargo.buildScripts.overrideCommand": [
        "cargo",
        "+nightly",
        "check",
        "--quiet",
        "--workspace",
        "--message-format=json",
    ],
    "rust-analyzer.cargo.noDefaultFeatures": true,
    // For Raspberry Pi 3
    "rust-analyzer.cargo.features": [
        "aarch64",
        "raspi3"
    ],
